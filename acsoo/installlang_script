iso_code =  "%(lang)s"
lang = env['res.lang'].search([('iso_code', '=', iso_code), ('active', '>=', 0)], limit=1)
vals = {
    'lang': lang.code,
    'overwrite': True,
}
wiz = env['base.language.install'].create(vals)
wiz.lang_install()